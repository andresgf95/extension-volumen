{
    "manifest_version": 3,
    "name": "Volume Amplifier",
    "version": "1.0",
    "description": "Aumenta el volumen del audio en la pestaña actual más allá del 100%.",
    "permissions": [
      "activeTab", 
      "scripting",   
      "storage"      
    ],
    "action": {
      "default_popup": "index.html", 
      "default_icon": {
        "16": "icons/icon16.png",
        "32": "icons/icon32.png",
        "192": "icons/android-chrome-192x192.png"
      }
    },
    "icons": {
      "16": "icons/icon16.png",
      "32": "icons/icon32.png",
      "192": "icons/android-chrome-192x192.png"
        },
     "background": {
      "service_worker": "background.js" 
     }
  }